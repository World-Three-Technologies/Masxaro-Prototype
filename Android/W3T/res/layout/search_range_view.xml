<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/search_range_layout"
    android:layout_width="fill_parent" android:layout_height="fill_parent"
    android:orientation="horizontal"
>
    <EditText android:id="@+id/start_date"
        android:hint="start" android:focusableInTouchMode="false"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_weight="1"
    />
    <EditText android:id="@+id/end_date"
        android:hint="end" android:focusableInTouchMode="false"
        android:layout_width="wrap_content" android:layout_height="wrap_content"
        android:layout_weight="1"
    />
</LinearLayout>
